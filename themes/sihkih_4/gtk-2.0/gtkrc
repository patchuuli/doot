	engine "murrine"{
		highlight_shade		= 0.96   		# set highlight amount for buttons or widgets
		lightborder_shade   	= 0.05 			# sets lightborder amount for buttons or widgets
		lightborderstyle    	= 1    			# 0 = lightborder on top side, 1 = lightborder on all sides
		profile			= MURRINE		# engine style options: CANDIDO, CLEARLOOKS, MIST, MURRINE, NODOKA		
		contrast 		= 0.1
		gradient_shades 	= {.5,0.8,0.8,1.01}
		glow_shade          	= 1.00 			# 0.0:0.1:2.0
   		glowstyle           	= 4			# 0 = top glow, 1 = bottom glow, 2 = top and bottom glow, 3 = horizontal glow, 4 = centered glow
		roundness		= 3
		rgba			= TRUE
	}}
