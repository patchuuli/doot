	engine "nodoka"
	{	style			= MODERN
		animation		= TRUE 			# FALSE = disabled, TRUE = enabled
		gradients		= TRUE
		roundness		= 4 			# 0 = squared, 1 = old default, more will increase roundness
		stripes			= TRUE			# enable or disable stripes in progressbars
