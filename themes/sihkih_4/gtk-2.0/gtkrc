	fg[NORMAL] 			= mix(0.25, shade(0.20,@bg_color), shade(0.40,@selected_bg_color))
	fg[PRELIGHT] 			= shade (1.55,@selected_fg_color)
	fg[SELECTED] 			= @selected_fg_color
	fg[ACTIVE] 			= mix (1.0, shade(0.95,@selected_fg_color), shade(0.05,@bg_color))
	fg[INSENSITIVE] 		= darker (shade(0.75, @bg_color))

	engine "murrine"{
		highlight_shade		= 1.00   		# set highlight amount for buttons or widgets
		lightborder_shade   	= 0.05 			# sets lightborder amount for buttons or widgets
		lightborderstyle    	= 1    			# 0 = lightborder on top side, 1 = lightborder on all sides
		profile			= MURRINE		# engine style options: CANDIDO, CLEARLOOKS, MIST, MURRINE, NODOKA		
		contrast 		= 0.5
