style "murrine-text-is-fg-color-workaround"
{	text[NORMAL]      = shade (0.1,@fg_color)
	text[PRELIGHT]    = @fg_color
	text[SELECTED]    = @selected_fg_color
	text[ACTIVE]      = @fg_color
	text[INSENSITIVE] = darker (@bg_color)
}
widget_class "*.<GtkComboBox>.<GtkCellView>"   style "murrine-comboboxentry"
style "murrine-text-is-fg-color-workaround-dark"
{	text[NORMAL] = shade(0.52,@bg_color)
}
widget_class "*Toolbar*.<GtkComboBox>.<GtkCellView>"   style "murrine-comboboxentry"
style "murrine-menuitem-text-is-fg-color-workaround" 
{	text[NORMAL]        = @bg_color
	text[ACTIVE]        = @bg_color
	text[PRELIGHT] = mix (0.4, @fg_color, '#ffffff')
}
widget "*.gtk-combobox-popup-menu.*"   style "murrine-comboboxentry"
style "murrine-fg-is-text-color-workaround"
{	fg[NORMAL]      = shade(1.0,@text_color)
	fg[PRELIGHT]    = @text_color
	fg[ACTIVE]      = @selected_fg_color
	fg[SELECTED]    = @selected_fg_color
	fg[INSENSITIVE] = darker (@bg_color)
}
