gtk_color_scheme = "fg_color:#0D0C0A\nbg_color:#C5C3B4\nbase_color:#E0DECE\ntext_color:#0D0C0A\nselected_bg_color:#7D8C76\nselected_fg_color:#FFFABB\ntooltip_bg_color:#FFFABB\ntooltip_fg_color:#262626"
style "murrine-default"
{	GtkButton      ::child-displacement-x = 1
	GtkButton      ::child-displacement-y = 1
	GtkButton      ::default-border       = { 0, 0, 0, 0 }
	GtkCheckButton ::indicator-size       = 14
	GtkPaned       ::handle-size          = 6
	GtkRange       ::trough-border        = 0
	GtkRange       ::slider-width         = 15
	GtkRange       ::stepper-size         = 15
	GtkScale       ::slider-length        = 18
	GtkScale       ::trough-side-details  = 0	# 0 = thin slider, >0 = thick slider
	GtkScrollbar   ::min-slider-length    = 20
	GtkMenuBar     ::internal-padding     = 0
	GtkExpander    ::expander-size        = 16
	GtkToolbar     ::internal-padding     = 1
	GtkTreeView    ::expander-size        = 14
	GtkTreeView    ::vertical-separator   = 0
	GtkMenu        ::horizontal-padding   = 1
	GtkMenu        ::vertical-padding     = 0
	GtkToolbar	:: shadow-type		= GTK_SHADOW_IN
        GtkMenuBar      :: shadow-type          = GTK_SHADOW_OUT
	WnckTasklist   ::fade-overlay-rect    = 10

	xthickness = 0
	ythickness = 0

	fg[NORMAL]        		= shade(1.5,@fg_color)
	fg[PRELIGHT]      		= shade(1.6,@selected_fg_color)
	fg[SELECTED]      		= @selected_fg_color
	fg[ACTIVE]        		= shade(1.0,@fg_color)
	fg[INSENSITIVE]   		= darker(mix(1.0,shade (0.95, @bg_color),shade(0.5,@text_color)))

	bg[NORMAL]        		= @bg_color
	bg[PRELIGHT]      		= mix (0.05, shade(0.6,@base_color), shade(0.85,@selected_bg_color))
	bg[SELECTED]	  		= @selected_bg_color
	bg[INSENSITIVE]   		= darker(@bg_color)
	bg[ACTIVE]        		= shade (0.8, @bg_color)

	base[NORMAL]      		= @base_color
	base[PRELIGHT]    		= mix (1.85, shade(0.49,@base_color), shade(1.35,@selected_bg_color))
	base[ACTIVE]      		= darker(shade(0.5,@selected_bg_color))
	base[SELECTED]    		= @selected_bg_color
	base[INSENSITIVE] 		= mix (0.90, @bg_color,shade (1.0,@base_color))

	text[NORMAL]      		= @text_color
	text[PRELIGHT]    		= shade (1.7,@selected_fg_color)
	text[ACTIVE]      		= shade (1.5,@selected_fg_color)
	text[SELECTED]    		= @selected_fg_color
	text[INSENSITIVE] 		= darker(mix(1.0,shade (0.95, @bg_color),shade(0.5,@text_color)))

	engine "murrine" 
	{	animation           	= TRUE  			# FALSE = disabled, TRUE = enabled
		contrast            	= 0.05   			# 0.8 for less contrast, more than 1.0 for more contrast on borders
		glazestyle          	= 3    				# 0 = flat highlight, 1 = curved highlight, 2 = concave style, 3 = top curved highlight, 4 = beryl highlight
		gradient_shades    	= {0.65,0.60,0.65,0.60} 	# default: {1.1,1.0,1.0,1.1}
		gradients           	= TRUE  			# FALSE = disabled, TRUE = enabled
		lightborderstyle    	= 1    				# 0 = lightborder on top side, 1 = lightborder on all sides
		listviewheaderstyle 	= 1     			# 0 = flat, 1 = glassy, 2 = raised
		listviewstyle      	= 1     			# 0 = nothing, 1 = dotted
		menustyle           	= 0     			# 0 = no vertical menu stripe, 1 = display vertical menu stripe
		reliefstyle	    	= 1     			# 0 = flat, 1 = inset, 2 = shadow
		rgba		    	= TRUE  			# FALSE = disabled, TRUE = enabled
		roundness           	= 1	    			# 0 = squared, 1 = old default, more will increase roundness
		sliderstyle         	= 1   				# 0 = nothing added, 1 = handles
		stepperstyle        	= 1     			# 0 = standard, 1 = integrated stepper handles, 2 = unknown
		profile		    	= MURRINE 			# engine style options: CANDIDO, CLEARLOOKS, MIST, MURRINE, NODOKA
		toolbarstyle	    	= 1     			# 0 = flat, 1 = glassy, 2 = gradient
	}}
style "murrine-wide"
{
	xthickness = 1
	ythickness = 1
}
style "murrine-wider"
{
	xthickness = 2
	ythickness = 2
	}
style "murrine-menubar"
{	xthickness = 2
	ythickness = 2

	bg[NORMAL] 			=shade(0.75, @bg_color)


	engine "murrine"{
		menuitemstyle      	= 2   				# 0 = flat, 1 = glassy, 2 = striped
		menubarstyle 		= 2     			# 0 = flat, 1 = glassy, 2 = gradient, 3 = striped
		menubaritemstyle    	= 1    				# 0 = menuitem look, 1 = button look
		contrast 		= 0.10

	}}
