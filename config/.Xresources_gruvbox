Xft*antialias: 				True
URxvt.letterSpace:      	0
Xft.autohint:   			0
!Xft.dpi:        			96
Xft.hinting:    			True
Xft.hintstyle:  			hintfull
Xft.lcdfilter:  			lcddefault
Xft.rgba:       			none
	


!URxvt.keysym.Home:         \033[1~
!URxvt.keysym.End:          \033[4~
